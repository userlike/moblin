{
  "name": "@moblin/chakra-ui",
  "version": "0.6.0",
  "description": "Moblin layout system for chakra-ui",
  "author": "Anil Anar",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "typings": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production concurrently npm:build:*",
    "test": "jest --env=jsdom --passWithNoTests",
    "lint": "eslint src --ext .ts,.tsx",
    "prepare": "npm run build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build:esm": "cross-env TARGET_ENV=esm babel src --extensions .ts,.tsx -d dist/esm --source-maps",
    "build:cjs": "cross-env TARGET_ENV=cjs babel src --extensions .ts,.tsx -d dist/cjs --source-maps",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist/types",
    "rush": "node ../../common/scripts/install-run-rush.js",
    "lint-staged": "lint-staged"
  },
  "peerDependencies": {
    "react": ">=18",
    "@chakra-ui/react": "^2.0.0",
    "@chakra-ui/system": "^2.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix",
    "*.{js,jsx,ts,tsx,json,md}": "prettier --write"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/runtime": "^7.15.4",
    "@chakra-ui/react": "^2.2.1",
    "@chakra-ui/system": "^2.1.3",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@jest/globals": "^28.1.1",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@testing-library/react": "^13.3.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-loader": "^8.2.2",
    "chromatic": "^6.6.3",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "csstype": "^3.1.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-testing-library": "^5.5.1",
    "framer-motion": "6.3.3",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "require-from-string": "^2.0.2",
    "rimraf": "^3.0.2",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "webpack": "^5.73.0"
  }
}
