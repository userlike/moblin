{
  "compilerOptions": {
    "incremental": true,
    "target": "ES2015",
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "lib": ["dom", "esnext"],
    "importHelpers": false,
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
}
